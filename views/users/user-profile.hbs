{{#if message}}
<div class="all-messages">{{message}}</div>
{{/if}}

<h2>User's profile</h2>

<a href="/home">User Home</a>
<br>
<a href="/edit-infos/{{data._id}}">Edit account information</a>

<img src="{{data.logo}}" alt="profile-picture">

<div>Username: {{data.username}}</div>
<br>
<div>Company: {{data.name}}</div>
<br>
<div>Sector: {{data.sector}}</div>
<br>

<div>CNPJ: {{data.cnpj}}</div>
<br>

<div>Address: {{data.address}}</div>
<br>

<div>Phone: {{data.phone}}</div>





